<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Tax payers Registration</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <link rel="apple-touch-icon" sizes="76x76" href="~/assets/img/FB_IMG_1453207869355-removebg-preview.png" />
    <link rel="icon" type="image/png" href="~/assets/img/FB_IMG_1453207869355-removebg-preview.png" />

    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />

    <!-- CSS Files -->
    <link href="~/assets/css/demo.css" rel="stylesheet" />
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.23/css/jquery.dataTables.min.css">
    <link href="~/assets/css/main.css" rel="stylesheet" />

    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="~/assets/css/demo.css" rel="stylesheet" />
</head>
<body>
    <nav class="navbar navbar-expand-sm sticky-top navbar-light bg-light">
        <div class="container">
            <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbar1">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbar1">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <img src="assets/img/Ogun WEB-LOGO.png" style="height:35px;width:150px">
                    </li>
                </ul>
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item active">
                        <a class="nav-link" href="#">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">About</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Contact</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="image-container set-full-height" style="background-image: url('../assets/img/ogun_state_house.jpg')">
        @RenderBody()
        <div class="footer">
            <div class="container text-center">
                Copyright <i class="fa fa-copyright"></i> @DateTime.Now.Year ICMA Proffessional, All rights reserved
            </div>
        </div>
    </div>


    <script src="~/assets/js/jquery-2.2.4.min.js" type="text/javascript"></script>
    <script src="~/assets/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="https://cdn.datatables.net/1.10.23/js/jquery.dataTables.min.js"></script>
    <script src="~/assets/js/jquery.bootstrap.js" type="text/javascript"></script>

    <!--  Plugin for the Wizard -->
    <script src="~/assets/js/material-bootstrap-wizard.js"></script>

    <!--  More information about jquery.validate here: http://jqueryvalidation.org/	 -->
    <script src="~/assets/js/jquery.validate.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#organisation').hide();
            $('#organisation2').hide();
            $('#organisation3').hide();
            $('#individual').hide();
            $('#individual2').hide();
            $('#individual3').hide();

            $('#search').on('change', function () {

                let k = $('#search').val();
                if (k === "Individual") {
                    $('#individual').show();
                    $('#organisation').hide();
                    $('#organisation2').hide();
                    $('#organisation3').hide();
                }
                if (k === "Organisation") {
                    $('#organisation').show();
                    //$('#individual').hide();
                    //$('#individual2').hide();
                    //$('#individual3').hide();
                }
                //  else{
                //     $('#organisation').hide();
                //     $('#individual').hide();
                //  }
            });
            $('#individual').on('change', function () {
                $('#individual2').show();
            });
            $('#organisation').on('change', function () {
                $('#organisation2').show();
            });
            $('#searchIndividual').click(function () {
                $('#individual3').show();
            });
            $('#searchOrganisation').click(function () {
                $('#organisation3').show();
            });
            $('#example').DataTable();
            $('#example2').DataTable();

        });
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
